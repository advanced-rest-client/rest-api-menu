{
  "name": "rest-api-menu",
  "version": "0.1.12",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A quick access menu for REST API projects",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "private": false,
  "main": [
    "rest-api-menu.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint rest-api-menu.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"rest-api-menu:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/rest-api-menu.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/rest-api-menu/issues",
    "email": "arc@mulesoft.com"
  }
}